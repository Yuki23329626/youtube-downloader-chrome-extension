{
  "name": "Light Youtube Downloder",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "youtube-downloader.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}